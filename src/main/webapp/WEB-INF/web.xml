<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>startServlet</servlet-name>
        <servlet-class>detective.StartServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>startServlet</servlet-name>
        <url-pattern>/startServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>finishServlet</servlet-name>
        <servlet-class>detective.FinishServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>finishServlet</servlet-name>
        <url-pattern>/finishServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CaseInvestigated</servlet-name>
        <jsp-file>/caseInvestigated.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>CaseInvestigated</servlet-name>
        <url-pattern>/caseInvestigated</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CaseNotInvestigated</servlet-name>
        <jsp-file>/caseNotInvestigated.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>CaseNotInvestigated</servlet-name>
        <url-pattern>/caseNotInvestigated</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Clue</servlet-name>
        <jsp-file>/clue.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Clue</servlet-name>
        <url-pattern>/clue</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FingerPrints</servlet-name>
        <jsp-file>/fingerprints.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>FingerPrints</servlet-name>
        <url-pattern>/fingerprints</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Witness</servlet-name>
        <jsp-file>/witness.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Witness</servlet-name>
        <url-pattern>/witness</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Suspect</servlet-name>
        <jsp-file>/suspect.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Suspect</servlet-name>
        <url-pattern>/suspect</url-pattern>
    </servlet-mapping>
</web-app>
